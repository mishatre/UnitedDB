			ЗаполнитьСвойствоXDTO(СведТов, "Акциз", Акциз, Истина, ТекстОшибки);
			
			РеквизитСтавкаНДС = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.СтавкаНДС");
