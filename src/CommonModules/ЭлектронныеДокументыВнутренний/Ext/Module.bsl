	Структура.Вставить("Количество", 0);
	Структура.Вставить("ЦенаДоКорректировки", 0);
	Структура.Вставить("Цена", 0);
	Структура.Вставить("СуммаДоКорректировки", 0);
	Структура.Вставить("Сумма", 0);
