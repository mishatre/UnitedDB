			// необязательные реквизиты:
			ЕдиницаИзмеренияКод = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.ЕдиницаИзмеренияКод");
			Если СтрДлина(ЕдиницаИзмеренияКод) = 3 Тогда
				ЗаполнитьСвойствоXDTO(СведТов, "ОКЕИ_Тов", ЕдиницаИзмеренияКод, , ТекстОшибки);
			Иначе
				ПоместитьВДопДанные("КодОКЕИ", ЕдиницаИзмеренияКод, Товар, "ТаблицаТоваров.НомерСтроки");
			КонецЕсли;
			
			Количество = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.Количество");
			ЗаполнитьСвойствоXDTO(СведТов, "КолТов", Количество, , ТекстОшибки);
			
			Цена = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.Цена");
			ЗаполнитьСвойствоXDTO(СведТов, "ЦенаТов", Цена, , ТекстОшибки);
			
			СуммаБезНДС = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.СуммаБезНДС");
			ЗаполнитьСвойствоXDTO(СведТов, "СтТовБезНДС", СуммаБезНДС, , ТекстОшибки);
			
			КодСтраны = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.КодСтраныПроисхождения");
			НомерДекларации = ЗначениеРеквизитаВДереве(Товар, "ТаблицаТоваров.НомерСтроки.НомерТаможеннойДекларации");
			
			ЕстьВладелецТД = Ложь;
			
			Если ФНС502 Тогда
				
				Если ТипЗнч(КодСтраны) = Тип("Массив") Тогда
					
					Для Каждого Код Из КодСтраны Цикл
						
						ВладелецТД = ПолучитьОбъектТипаCML("Файл.Документ.ТаблСчФакт.СведТов.СвТД", ПространствоИменСхемы);
